{
  "name": "deezeroom",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:clear": "npx expo start --clear",
    "start:tunnel": "npx expo start --tunnel",
    "start:tunnel:clear": "npx expo start --tunnel --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix:line-endings": "prettier --write . --end-of-line lf",
    "clean": "npx rimraf node_modules && npx rimraf package-lock.json && npx rimraf .expo && npx rimraf android && npx rimraf ios && npx rimraf web",
    "c:i": "npm run clean && npm install",
    "verify:deps": "npx expo install --check",
    "verify:lockfile": "npm ci --ignore-scripts --dry-run",
    "env:list": "eas env:list",
    "build:dev:android": "npm run lint:fix && eas build --profile development --platform android",
    "build:dev:ios": "npm run lint:fix && eas build --profile development --platform ios",
    "build:dev:web": "npm run lint:fix && eas build --profile development --platform web",
    "build:prod:android": "npm run lint:fix && eas build --profile production --platform android",
    "deploy:web:export": "npx expo export --platform web",
    "deploy:web": "eas deploy --pro"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "clsx": "^2.1.1",
    "expo": "^53.0.22",
    "expo-audio": "~0.4.9",
    "expo-auth-session": "^6.2.1",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-network": "~7.1.5",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-video": "~2.2.2",
    "expo-web-browser": "~14.2.0",
    "firebase": "^12.2.1",
    "graphql": "^16.11.0",
    "graphql-http": "^1.22.4",
    "lucide-react-native": "^0.542.0",
    "nativewind": "^4.1.23",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-alert-notification": "^0.4.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-swipe-modal-up-down": "^1.1.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.34.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
