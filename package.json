{
  "name": "deezeroom",
  "main": "expo-router/entry",
  "version": "1.1.54",
  "scripts": {
    "start": "expo start",
    "start:clear": "npx expo start --clear",
    "start:tunnel": "npx expo start --tunnel",
    "start:tunnel:clear": "npx expo start --tunnel --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix:line-endings": "prettier --write . --end-of-line lf",
    "clean": "npx rimraf node_modules && npx rimraf package-lock.json && npx rimraf .expo && npx rimraf android && npx rimraf ios && npx rimraf web",
    "c:i": "npm run clean && npm install",
    "verify:deps": "npx expo install --check",
    "verify:lockfile": "npm ci --ignore-scripts --dry-run",
    "doctor": "npx expo doctor",
    "env:list": "eas env:list",
    "build:dev:android": "npm run lint:fix && eas build --profile development --platform android",
    "build:dev:ios": "npm run lint:fix && eas build --profile development --platform ios",
    "build:dev:web": "npm run lint:fix && eas build --profile development --platform web",
    "build:prod:android": "npm run lint:fix && eas build --profile production --platform android",
    "deploy:web:export": "npx expo export --platform web",
    "deploy:web": "npm run deploy:web:export && eas deploy --prod"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "clsx": "^2.1.1",
    "@expo/html-elements": "^0.13.6",
    "expo": "~54.0.4",
    "expo-audio": "~1.0.11",
    "expo-auth-session": "~7.0.8",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.8",
    "expo-dev-client": "~6.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-network": "~8.0.7",
    "expo-router": "~6.0.2",
    "expo-splash-screen": "~31.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-video": "~3.0.11",
    "expo-web-browser": "~15.0.7",
    "firebase": "^12.2.1",
    "graphql": "^16.11.0",
    "graphql-http": "^1.22.4",
    "lucide-react-native": "^0.542.0",
    "nativewind": "^4.1.23",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-alert-notification": "^0.4.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-svg": "15.12.1",
    "react-native-swipe-modal-up-down": "^1.1.0",
    "react-native-webview": "13.15.0",
    "tailwindcss": "^3.4.17",
    "@expo/metro-runtime": "~6.1.2",
    "react-native-worklets": "0.5.1",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.34.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
