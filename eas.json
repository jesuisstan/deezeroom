{
  "cli": {
    "version": ">= 16.19.1",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": "GOOGLE_SERVICES_JSON"
      },
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": "GOOGLE_SERVICES_JSON",
        "EXPO_PUBLIC_APP_URL": "https://deezeroom.expo.app",
        "EXPO_PUBLIC_SERVER_URL": "https://deezeroom-server.vercel.app"
      },
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      }
    },
    "production": {
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": "GOOGLE_SERVICES_JSON",
        "EXPO_PUBLIC_APP_URL": "https://deezeroom.expo.app",
        "EXPO_PUBLIC_SERVER_URL": "https://deezeroom-server.vercel.app"
      },
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      }
    }
  }
}
